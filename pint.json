{
  "preset": "laravel",
  "rules": {
    "blank_line_after_opening_tag": true,
    "single_blank_line_at_eof": true,
    "no_extra_blank_lines": true,
    "single_import_per_statement": true,
    "ordered_imports": {
      "sort_algorithm": "alpha"
    }
  },
  "plugins": [
    "Shufo\\LaravelPint\\BladePlugin"
  ]
}
